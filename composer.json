{
    "name": "technoyer/larageo",
    "description": "Giving developers a hand to fetch the visitors geo information, user agent information, and the device information, easy to use and accepts contributor",
    "keywords": ["laravel geolocation", "laravel ip2location", "laravel ip country", "laravel ip city", "laravel ip region", "laravel latitude", "laravel longitude", "technoyer", "laravel device detect", "laravel browser detect", "laravel user agent", "laravel ip timezone", "laravel ip currency", "laravel ip language"],
    "homepage": "https://github.com/Technoyer/larageo",
    "support": {
        "issues": "https://github.com/Technoyer/larageo/issues",
        "source": "https://github.com/Technoyer/larageo"
    },
    "require": {
        "php": ">=8.0",
        "mobiledetect/mobiledetectlib": "^3.74.0"
    },
    "require-dev": {
        "orchestra/testbench": "6.x-dev",
        "guzzlehttp/guzzle": "7.5.x-dev"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Technoyer\\Larageo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Technoyer\\Larageo\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Mohamed Gomaa",
            "email": "senior.mgomaa@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Technoyer\\Larageo\\Providers\\LarageoServiceProvider"
            ],
            "aliases": {
                "Larageo": "Technoyer\\Larageo\\Facades\\Larageo"
            }
        }
    },
    "minimum-stability": "dev"
}
